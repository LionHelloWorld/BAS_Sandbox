{
  "_version": "1.65.0",
  "sap.app": {
    "id": "lion.zlionui5003",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.5",
      "toolsId": "a0e27fae-b8b4-4c6a-a296-6b3a398a2c20"
    },
    "dataSources": {
      "quotationRemote": {
        "uri": "/sap/opu/odata4/sap/zapi_lion_quotehdr_o4/srvd_a2x/sap/zui_lion_quotehdr_o4/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "zlion_ui5_quote-display": {
          "semanticObject": "zlion_ui5_quote",
          "action": "display",
          "title": "{{zlion_ui5_quote-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.133.3",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "lion.zlionui5003.i18n.i18n"
        }
      },
      "quotations": {
        "dataSource": "quotationRemote",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "lion.zlionui5003.view",
        "async": true,
        "viewPath": "lion.zlionui5003.view"
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": ":?query:",
          "target": [
            "TargetApp"
          ]
        }
      ],
      "targets": {
        "TargetApp": {
          "id": "App",
          "name": "App"
        }
      }
    },
    "rootView": {
      "viewName": "lion.zlionui5003.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
